<br/>
  <h2><center>Lista de usu치rios</center></h2>
<br/>
  <div class="col-sm-15" "panel panel-default">
    <div class="col-sm-3" "panel-heading">
      <ul class="nav nav-pills nav-stacked">
      <li class="active"><%= link_to t('buttons.home'),root_path %></li>
      <li class="danger"><%= link_to "Usu치rios", user_profile_index_path %></li>
      <li class="warning"></span><%= link_to "Equipes", teams_path %></li>
      <li class="success"><a href="#">Projetos</a></li>
      <li class="info"><a href="#">Relat칩rios</a></li>
      </ul>
    </div>
    <div class="col-sm-9" "panel-body">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title"><center>Usu치rios</center></h3>
        </div>
      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover ">
          <thead>
            <tr>
              <th><%= @users.model.human_attribute_name("student_number") %></th>
              <th><%= @users.model.human_attribute_name("name") %></th>
              <th><%= @users.model.human_attribute_name("email") %></th>
              <th colspan="2"></th>
            </tr>
          </thead>
        <tbody>
          <% @users.each do |user_profile| %>
            <tr>
              <td><%= user_profile.student_number %></td>
              <td><%= user_profile.name %></td>
              <td><%= user_profile.user.email %></td>
              <td><%= user_profile.user.password %></td>
              <td>
                <%= link_to "Visualizar", user_profile_path(user_profile.user) %>
              </td>
              <td>
                <% if can? :update, User %>
                  <%= link_to t('buttons.edit'), edit_user_profile_path(user_profile.user) %>
                <% end %>
              </td>

              <td>
                <% if can? :destroy, User %>
                  <%= link_to t('buttons.destroy'), user_profile_path(user_profile.user), method: :delete, data: { confirm: t('messages.confirm') } %>
                <% end %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <br>
      <%= paginate @users %>
      <br>
      <%= link_to t('buttons.new'), new_user_profile_path, class: "btn btn-primary" %> |
      <%= link_to t('buttons.back'),root_path, class: "btn btn-success" %>
      </div>
    </div>
  </div>
